<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.fiend.entity.Appointment" table="appointment" schema="carqxdb">
        <id name="userId" column="userId"/>
        <property name="customerName" column="customerName"/>
        <property name="sex" column="sex"/>
        <property name="carNumber" column="carNumber"/>
        <property name="vehicleId" column="vehicleId"/>
        <property name="tel" column="tel"/>
        <property name="appointmentTime" column="appointmentTime"/>
        <property name="serviceSmallId" column="serviceSmallId"/>
    </class>
</hibernate-mapping>