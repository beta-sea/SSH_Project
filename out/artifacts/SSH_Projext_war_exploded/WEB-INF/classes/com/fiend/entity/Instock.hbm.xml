<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.fiend.entity.Instock" table="instock" schema="carqxdb">
        <id name="id" column="id"/>
        <property name="inventor" column="Inventor"/>
        <property name="inTime" column="inTime"/>
        <property name="fittingId" column="fittingId"/>
        <property name="inNumber" column="inNumber"/>
    </class>
</hibernate-mapping>