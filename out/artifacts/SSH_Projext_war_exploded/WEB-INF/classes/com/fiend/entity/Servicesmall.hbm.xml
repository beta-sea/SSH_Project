<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.fiend.entity.Servicesmall" table="servicesmall" schema="carqxdb">
        <id name="serviceSmallId" column="serviceSmallId"/>
        <property name="name" column="name"/>
        <property name="price" column="price"/>
        <property name="serviceIntroduce" column="serviceIntroduce"/>
        <property name="serviceType" column="serviceType"/>
    </class>
</hibernate-mapping>