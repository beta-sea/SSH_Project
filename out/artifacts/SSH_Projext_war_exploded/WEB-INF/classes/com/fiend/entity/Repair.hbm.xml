<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.fiend.entity.Repair" table="repair" schema="carqxdb">
        <id name="id" column="id"/>
        <property name="customerId" column="customerId"/>
        <property name="serviceTime" column="serviceTime"/>
        <property name="lengthService" column="lengthService"/>
        <property name="staffId" column="staffId"/>
        <property name="cost" column="cost"/>
        <property name="deliveryTime" column="deliveryTime"/>
        <property name="serviceSmallId" column="serviceSmallId"/>
    </class>
</hibernate-mapping>